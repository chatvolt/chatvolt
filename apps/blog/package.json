{
  "name": "blog",
  "version": "2.0.0",
  "private": true,
  "description": "The perfect starter kit for building beautiful websites with Next.js and Notion.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/nextjs-notion-starter-kit",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dotenv": "dotenv -e ../../.env.local",
    "dev": "npm run dotenv -- next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel deploy",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint '**/*.{ts,tsx}'",
    "test:prettier": "prettier --write '**/*.{js,jsx,ts,tsx}' --check",
    "lint": "pnpm test:lint",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@fisch0920/use-dark-mode": "^2.4.0",
    "@keyvhq/core": "^1.6.9",
    "@keyvhq/redis": "^1.6.10",
    "@react-icons/all-files": "^4.1.0",
    "@vercel/og": "^0.5.11",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "expiry-map": "^2.0.0",
    "fathom-client": "^3.6.0",
    "got": "^12.0.3",
    "isomorphic-unfetch": "^3.1.0",
    "lqip-modern": "^2.0.0",
    "next": "14.2",
    "notion-client": "^6.15.6",
    "notion-types": "^6.15.6",
    "notion-utils": "^6.15.6",
    "p-map": "^5.5.0",
    "p-memoize": "^6.0.1",
    "patch-package": "^8.0.0",
    "posthog-js": "^1.88.4",
    "react": "18.2.0",
    "react-body-classname": "^1.3.1",
    "react-dom": "18.2.0",
    "react-notion-x": "^6.15.6",
    "react-tweet-embed": "^2.0.0",
    "react-use": "^17.4.2",
    "rss": "^1.2.2",
    "tailwindcss": "^3.3.6"
  },
  "devDependencies": {
    "@chatvolt/config-tailwind": "workspace:*",
    "@chatvolt/lib": "workspace:*",
    "@chatvolt/ui": "workspace:*",
    "@next/bundle-analyzer": "^13.3.0",
    "@types/node": "18.15.10",
    "@types/react": "18.0.29",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^7.1.0",
    "eslint-config-custom": "workspace:*",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "typescript": "5.0.2"
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  }
}
